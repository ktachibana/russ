{
  "private": true,
  "name": "russ",
  "version": "1.0.0",
  "description": "== README",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm run clean && npm run build-js && npm run build-css && cd ../public/assets && gzip -k *.js *.css",
    "clean": "rm -rf ../public/assets && mkdir -p ../public/assets",
    "build-js": "NODE_ENV=production webpack-cli -p",
    "build-css": "NODE_ENV=production stylus -I node_modules --include-css < frontend/application.styl | cleancss > ../public/assets/application.css",
    "watch": "concurrently 'npm run watch-js' 'npm run watch-css'",
    "watch-js": "webpack-cli -w --mode development",
    "watch-css": "stylus --watch -I node_modules --include-css application.styl -o ../public/assets/application.css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3",
    "bootstrap-tokenfield": "^0.12.0",
    "brace-expansion": "^1.1.7",
    "classnames": "^2.2.5",
    "clean-css": "^4.1.11",
    "clean-css-cli": "^4.1.11",
    "concurrently": "^3.5.1",
    "eventemitter2": "^5.0.1",
    "expose-loader": "^0.7.1",
    "hoek": ">=5.0.3",
    "jquery": "^3.2.1",
    "moment": "^2.13.0",
    "npm": "^5.8.0",
    "npm-check-updates": "^2.14.1",
    "pathjs": "^0.8.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "stylus": "^0.54.5",
    "underscore": "^1.8.3",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  }
}
