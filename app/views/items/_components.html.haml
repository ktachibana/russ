%script#item-panel{ type: 'text/x-template' }
  .panel.panel-default.item
    .panel-heading
      %a{ target: '_blank', v: { attr: 'href:link' } }
        {{title}}
      .pull-right{ v: { if: '!hidefeed' } }
        %small
          %a{ v: { attr: 'href: subscriptionPath' } }
            {{feed.usersSubscription.userTitle}}
      .clearfix
    .panel-body
      %p{ v: { html: 'description' } }
      .pull-right
        %span.glyphicon.glyphicon-upload
        %i {{publishedAtDate | date}}
      .clearfix
