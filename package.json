{
  "private": true,
  "name": "russ",
  "version": "1.0.0",
  "description": "== README",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm run clean && npm run build-js && npm run build-css && cd public/assets && gzip -k *.js *.css",
    "clean": "rm -rf public/assets && mkdir -p public/assets",
    "build-js": "cd frontend && NODE_ENV=production webpack -p",
    "build-css": "NODE_ENV=production stylus -I node_modules --include-css < frontend/application.styl | cleancss > public/assets/application.css",
    "watch": "concurrently 'npm run watch-js' 'npm run watch-css'",
    "watch-js": "cd frontend && webpack -w",
    "watch-css": "stylus --watch -I node_modules --include-css frontend/application.styl -o public/assets/application.css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.6",
    "bootstrap-tokenfield": "^0.12.0",
    "classnames": "^2.2.5",
    "clean-css": "^3.4.18",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "concurrently": "^2.1.0",
    "eventemitter2": "^1.0.5",
    "expose-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "moment": "^2.13.0",
    "pathjs": "^0.8.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "stylus": "^0.54.5",
    "underscore": "^1.8.3",
    "webpack": "^1.12.11"
  }
}
